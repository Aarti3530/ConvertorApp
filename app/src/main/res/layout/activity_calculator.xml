<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    tools:context=".Calculator"
    android:layout_height="match_parent"
    android:background="@drawable/wewe">

    <EditText
        android:id="@+id/pannel"
        android:numeric="integer"
        android:hint="Calculate"
        android:textSize="25dp"
        android:layout_width="350dp"
        android:layout_height="80dp"
        android:textColorHint="#262626"
        android:fontFamily="@font/one"
        android:textAppearance="@style/TextStyle"
        android:textColor="#262626"
        android:layout_marginTop="80dp"
        android:textAlignment="center"
        android:background="@color/teal_200"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.508"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/num9"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:fontFamily="@font/one"
        android:text="9"
        android:textSize="30sp"
        android:textAppearance="@style/TextStyle"
        android:textColor="#262626"
        app:layout_constraintEnd_toStartOf="@+id/num8"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/pannel"
        app:layout_constraintTop_toTopOf="@+id/num8" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/num8"
        android:textSize="30sp"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:layout_marginTop="52dp"
        android:layout_marginEnd="16dp"
        android:fontFamily="@font/one"
        android:text="8"
        android:textAppearance="@style/TextStyle"
        android:textColor="#262626"
        app:layout_constraintEnd_toStartOf="@+id/num7"
        app:layout_constraintTop_toBottomOf="@+id/pannel" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/num7"
        android:textSize="30sp"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:layout_marginTop="52dp"
        android:layout_marginEnd="92dp"
        android:fontFamily="@font/one"
        android:text="7"
        android:textAppearance="@style/TextStyle"
        android:textColor="#262626"
        app:layout_constraintEnd_toEndOf="@+id/pannel"
        app:layout_constraintTop_toBottomOf="@+id/pannel" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/num6"
        android:textSize="30sp"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:layout_marginTop="40dp"
        android:fontFamily="@font/one"
        android:text="6"
        android:textAppearance="@style/TextStyle"
        android:textColor="#262626"
        app:layout_constraintEnd_toStartOf="@+id/num5"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/num9"
        app:layout_constraintTop_toBottomOf="@+id/num9" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/num5"
        android:layout_width="75dp"
        android:textSize="30sp"
        android:layout_height="75dp"
        android:layout_marginTop="40dp"
        android:fontFamily="@font/one"
        android:text="5"
        android:textAppearance="@style/TextStyle"
        android:textColor="#262626"
        app:layout_constraintEnd_toStartOf="@+id/num4"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/num8"
        app:layout_constraintTop_toBottomOf="@+id/num8" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/num4"
        android:layout_width="75dp"
        android:textSize="30sp"
        android:layout_height="75dp"
        android:layout_marginTop="40dp"
        android:fontFamily="@font/one"
        android:text="4"
        android:textAppearance="@style/TextStyle"
        android:textColor="#262626"
        app:layout_constraintEnd_toEndOf="@+id/num7"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/num7"
        app:layout_constraintTop_toBottomOf="@+id/num7" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/num3"
        android:layout_width="75dp"
        android:textSize="30sp"
        android:layout_height="75dp"
        android:layout_marginTop="40dp"
        android:fontFamily="@font/one"
        android:text="3"
        android:textAppearance="@style/TextStyle"
        android:textColor="#262626"
        app:layout_constraintEnd_toStartOf="@+id/num2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/num6"
        app:layout_constraintTop_toBottomOf="@+id/num6" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/num2"
        android:layout_width="75dp"
        android:textSize="30sp"
        android:layout_height="75dp"
        android:layout_marginTop="40dp"
        android:fontFamily="@font/one"
        android:text="2"
        android:textAppearance="@style/TextStyle"
        android:textColor="#262626"
        app:layout_constraintEnd_toStartOf="@+id/num1"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/num5"
        app:layout_constraintTop_toBottomOf="@+id/num5" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/num1"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:layout_marginTop="40dp"
        android:fontFamily="@font/one"
        android:text="1"
        android:textSize="30sp"
        android:textAppearance="@style/TextStyle"
        android:textColor="#262626"
        app:layout_constraintStart_toStartOf="@+id/num7"
        app:layout_constraintTop_toBottomOf="@+id/num4" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/C"
        android:layout_width="80dp"
        android:layout_height="67dp"
        android:layout_marginEnd="28dp"
        android:background="@drawable/circular"
        android:fontFamily="@font/one"
        android:text="C"
        android:textSize="30sp"
        android:textAppearance="@style/TextStyle"
        android:textColor="#262626"
        app:layout_constraintBottom_toBottomOf="@+id/num7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/num7"
        app:layout_constraintVertical_bias="0.0" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/add"
        android:layout_width="80dp"
        android:layout_height="67dp"
        android:layout_marginStart="12dp"
        android:background="@drawable/circular"
        android:fontFamily="@font/one"
        android:text="+"
        android:textAppearance="@style/TextStyle"
        android:textColor="#262626"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="@+id/num4"
        app:layout_constraintEnd_toEndOf="@+id/C"
        app:layout_constraintStart_toEndOf="@+id/num4"
        app:layout_constraintTop_toTopOf="@+id/num4"
        app:layout_constraintVertical_bias="0.625" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/sub"
        android:layout_width="80dp"
        android:layout_height="67dp"
        android:background="@drawable/circular"
        android:fontFamily="@font/one"
        android:text="-"
        android:textAppearance="@style/TextStyle"
        android:textColor="#262626"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="@+id/num1"
        app:layout_constraintEnd_toEndOf="@+id/add"
        app:layout_constraintStart_toStartOf="@+id/add"
        app:layout_constraintTop_toTopOf="@+id/num1" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/mul"
        android:layout_width="80dp"
        android:layout_height="67dp"
        android:layout_marginTop="40dp"
        android:background="@drawable/circular"
        android:fontFamily="@font/one"
        android:text="x"
        android:textAlignment="center"
        android:textAppearance="@style/TextStyle"
        android:textColor="#262626"
        android:textSize="30sp"
        app:layout_constraintEnd_toStartOf="@+id/num0"
        app:layout_constraintHorizontal_bias="0.596"
        app:layout_constraintStart_toStartOf="@+id/num3"
        app:layout_constraintTop_toBottomOf="@+id/num3" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/num0"
        android:layout_width="80dp"
        android:layout_height="67dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="40dp"
        android:background="@drawable/circular"
        android:fontFamily="@font/one"
        android:text="0"
        android:textSize="30sp"
        android:textAppearance="@style/TextStyle"
        android:textColor="#262626"
        app:layout_constraintBottom_toBottomOf="@+id/mul"
        app:layout_constraintStart_toStartOf="@+id/num2"
        app:layout_constraintTop_toBottomOf="@+id/num2"
        app:layout_constraintVertical_bias="0.0" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/div"
        android:layout_width="80dp"
        android:layout_height="67dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="44dp"
        android:background="@drawable/circular"
        android:fontFamily="@font/one"
        android:text="/"
        android:textSize="30sp"
        android:textAppearance="@style/TextStyle"
        android:textColor="#262626"
        app:layout_constraintBottom_toBottomOf="@+id/num0"
        app:layout_constraintStart_toStartOf="@+id/num1"
        app:layout_constraintTop_toBottomOf="@+id/num1"
        app:layout_constraintVertical_bias="1.0" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/equal"
        android:text="="
        android:textSize="30dp"
        android:fontFamily="@font/one"
        android:textAppearance="@style/TextStyle"
        android:textColor="#262626"
        android:layout_width="350dp"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/num0" />


</androidx.constraintlayout.widget.ConstraintLayout>